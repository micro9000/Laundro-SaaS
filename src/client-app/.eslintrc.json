{
  "extends": ["next/core-web-vitals", "prettier"],
  "plugins": ["check-file"],
  "rules": {
    "prefer-arrow-callback": ["error"],
    "prefer-template": ["error"],
    "semi": ["error"],
    "quotes": ["error", "single"],
    "check-file/filename-naming-convention": [
      "error",
      {
        "src/**/": "CAMEL_CASE"
      },
      {
        "ignoreMiddleExtensions": true
      }
    ]
    // "check-file/folder-naming-convention": [
    //   "error",
    //   {
    //     "src/**/": "CAMEL_CASE"
    //   }
    // ]
  }
}